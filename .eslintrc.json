{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", {  
      "endOfLine" : "auto"
    }],
    "react/jsx-filename-extension": [
      1,
      {
          "extensions": [ ".js", ".jsx" ] 
      }
    ]
  }
}