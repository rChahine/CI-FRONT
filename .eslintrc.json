{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", {  
      "endOfLine" : "auto",
      "no-console": "off"
    }],
    "react/jsx-filename-extension": [
      1,
      {
          "extensions": [ ".js", ".jsx" ] 
      }
    ]
    
  }
}